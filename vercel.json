{
    "version": 2,
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.php"
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "18.x"
        }
      }
    ]
  }